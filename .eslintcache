\n    \n    const [props, set] = useSpring(() => ({ xy: [0, 0], config: { mass: 10, tension: 550, friction: 140 }}));\n\n    // useEffect(() => {\n    //     setTimeout(() => {\n    //         setItems([logoContainer()]);\n    //     //   items.current = [logoContainer()];\n    //     //   console.log(items.current);\n    //     }, 2000);\n    // },[]);\n\n    return (\n        <div className=\"header-container\" id=\"header-hash\" onMouseMove={({ clientX: x, clientY: y }) => set({ xy: calc(x, y)})}>\n            <animated.div className=\"header-background\" style={{ transform: props.xy.interpolate(trans1) }}/>\n            <div className=\"header-content\">\n                <animated.div className=\"typing-comp\" style={{ transform: props.xy.interpolate(trans2) }}>\n                    <TypingComponent1/>\n                    {transitions.map(({ item, props: { innerHeight, ...rest }, key }) => (\n                        <animated.div className=\"transitions-item\" key={key} style={rest}>\n                        <animated.div style={{height: innerHeight, display:'table', width:'100%' }}>{item}</animated.div>\n                        </animated.div>\n                    ))}\n                    <TypingComponent2/>\n                </animated.div>\n                <div className=\"typing-comp2\">\n                    <TypingComponent1/>\n                    <TypingComponent2/>\n                    <div id=\"logo-container\">\n                        <img src={logo} alt=\"logo\" id=\"logo\"/>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Header","/home/sophia/Coding/Portfolio/react_portfolio2/sophia_portfolio/src/components/About/About.js",[],"/home/sophia/Coding/Portfolio/react_portfolio2/sophia_portfolio/src/App.js",["65"],"/home/sophia/Coding/Portfolio/react_portfolio2/sophia_portfolio/src/index.js",[],"/home/sophia/Coding/Portfolio/react_portfolio2/sophia_portfolio/src/components/Skills/Skills.j